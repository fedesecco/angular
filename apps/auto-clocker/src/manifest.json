{
    "manifest_version": 3,
    "name": "Auto Clocker",
    "description": "Automatically clock in and out",
    "version": "0.0.1",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "action": {
        "default_title": "Auto Clocker",
        "default_popup": "index.html"
    },
    "permissions": ["tabs"],
    "host_permissions": ["*://*/*"]
}
